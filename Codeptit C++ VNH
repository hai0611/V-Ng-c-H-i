ĐẦU CUỐI GIỐNG NHAU
#include<bits/stdc++.h>
using namespace std;
//cach1
int checkse(string s){
return(s[0]==s[s.size()-1]);
}
int countcase(string s){
    int dem=0;
for(int i=0;i<s.size();i++)
    for(int j=1;j<=s.size()-i;j++)
    if(checkse(s.substr(i,j))) dem+=1;
return dem;
}
using namespace std;
int main(){
    int test;
    cin>>test;
    string str;
    while(test--){
    cin>>str;
    cout<<countcase(str)<<endl;
    } }
KÝ TỰ KHÔNG LẶP
#include<bits/stdc++.h>
using namespace std;
using namespace std;
int main(){
    int test;
    cin>>test;
    string str;
    while(test--){
    bool check[100000]={0};
    cin>>str;
    for(int i=0;i<str.size();i++)
        for(int j=i+1;j<str.size();j++)
            if(str[j]==str[i]) {
                    check[i]=1;
                    check[j]=1;}

    for(int i=0;i<str.size();i++)
        if(check[i]==0) cout<<(char)str[i];
    cout<<endl;
    } }
 NHỎ NHẤT - LỚN NHẤT
 #include<bits/stdc++.h>
using namespace std;
string minn(int m,int s){
    vector<int>k(m);
    string kq="";
    if(s > 9*m || (m>1 && s ==0)) return "-1";
    s-=1;
    for(int i=m-1;i>0;i--){
        if(s > 9) {k[i] = 9 ; s-=9;}
        else {k[i] = s ; s=0;}
    }
    k[0]=s+1;
    for(int i=0;i<m;i++){
        kq.push_back(k[i]+'0');
    }
    return kq;
}
string maxx(int m,int s){
    vector<int>k(m);
    string kq="";
    if(s > 9*m || (m>1 && s ==0)) return "-1";
    for(int i=0;i<m;i++){
        if(s > 9) {k[i] = 9 ; s-=9;}
        else {k[i] = s ; s=0;}
    }
    for(int i=0;i<m;i++){
        kq.push_back(k[i]+'0');
    }
    return kq;
}
int main() {
    int m, s;
    cin>>m>>s;
    cout<<minn(m,s)<<" "<<maxx(m,s);
}
SỐ ĐẸP
#include<bits/stdc++.h>
using namespace std;
int checkcl(string s){
    for(int i=0;i<s.size();i++)
        if((s[i]-'0')%2 != 0) return 0;
    return 1;
}
int checktn(string s){
int dem=0;
int n=s.size();
for(int i=0;i<n/2;i++){
        if(s[i]==s[n-i-1]) dem+=1;}
    if(dem==n/2 && checkcl(s)==1) return 1;
    return 0;
}
int main(){
    int t;
    cin>>t;
    cin.ignore();
    while(t--){
    string s;
    getline(cin,s);
    if(checkcl(s)==1 && checktn(s) ==1) cout<<"YES\n";
    else cout<<"NO\n";
    }}
