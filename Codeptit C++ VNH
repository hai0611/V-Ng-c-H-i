-- SỐ MAY MẮN
#include <iostream>
#include<math.h>
using namespace std;

int main()
{
    int a;
    cin>>a;
    while(a>0){
    long k;
    cin>>k;
    int n[5];
    for(int i=1;i<=2;i++){

    n[i]=k%10;
    k/=10;
    }
    //cout<<n[2]*10+n[1]<<endl;
    if(n[2]*10+n[1]==86) cout<<1<<endl;
    else cout<<0<<endl;
    a--;
   }
    return 0;
}
--Tính tổng chữ số
#include <iostream>
#include<math.h>
using namespace std;
int tongcacchuso(int a,int tong){
    while(a>0){
    int k;
    k=a%10;
    a/=10;
    tong+=k;
        }
    return tong;
}
int main()
{
    int a;
    cin>>a;
    while(a>0){
    long k;
    cin>>k;
    while(k>=10){
    k=tongcacchuso(k,0);}
    cout<<k<<endl;
    a--;
   }
    return 0;
}
-- TÍNH TỔNG PHÂN THỨC
#include <iostream>
#include<math.h>
#include <iomanip>
using namespace std;

int main()
{

    long n;
    cin>>n;
    double tong=0;
    for(int i=1;i<=n;i++){
        tong+=(float)1/i;

    }
cout<<fixed<<setprecision(4)<<tong;;

}
--Chữ hoa chữ thường 
#include <iostream>
#include<math.h>
#include<string.h>
using namespace std;

int main()
{
    int a;
    cin>>a;
    while(a>0){
    char k;
    cin>>k;
    if(isupper(k)) cout<<(char)tolower(k)<<endl;
    if(islower(k)) cout<<(char)toupper(k)<<endl;


    a--;
   }
    return 0;
}
--TÍNH TỔNG 1 ĐẾN N
#include <iostream>
#include<math.h>
#include<string.h>
using namespace std;

int main()
{
    int a;
    cin>>a;
    while(a>0){
    long n;
    cin>>n;
    long tong=0;
    tong=(n+1)*n/2;
cout<<tong<<endl;
    a--;
   }
    return 0;
}
-- TÍNH TỔNG GIAI THỪA
#include <iostream>
#include<math.h>
using namespace std;

int main()
{

    long n;
    cin>>n;
    long sum=0,gt=1;
    for(int i=1;i<=n;i++){
    gt*=i;
    sum+=gt;

    }
cout<<sum;

}
Số lộc phát
#include <iostream>
#include<math.h>
#include<string.h>
#include <iomanip>
using namespace std;
int KT(int k){
    while(k>0){
            int so;
            so=k%10;
            k=k/10;
            if(so!=0 && so!=6 && so!=8) return 0 ;
            }
            return 1;
}
int main()
{

    int n;
    cin>>n;
    while(n>0){
        int k;
        cin>>k;
        if(KT(k)==1) cout<<"YES"<<endl;
        else cout<<"NO"<<endl;
        n--;
    }


}
 SỐ THUẬN NGHỊCH
 #include <iostream>
#include<math.h>
#include<string.h>
#include <iomanip>
using namespace std;

int main()
{

    int n;
    cin>>n;
    while(n>0){
        char k[1000];
        cin>>k;
        int dem=0;
        for(int i=0;i<strlen(k);i++){
            if(k[i]!=k[strlen(k)-1-i]) dem+=1;
        }
        if(dem==0) cout<<"YES"<<endl;
        else cout<<"NO"<<endl;
        n--;
    }


}
 CHẤM ĐIỂM TRẮC NGHIỆM
 #include <iostream>
#include<math.h>
#include<string.h>
#include <iomanip>
using namespace std;

int main()
{

    int n;
    cin>>n;
    while(n>0){
            int a;
            float diem=0;
        cin>>a;
        char dapan[20];
        for(int i=0;i<15;i++){
        cin>>dapan[i];}
        if(a==101){
            char k[101]={'A','B','B','A','D','C','C','A','B','D','C','C','A','B','D'};
            for(int i=0;i<15;i++){
                if(dapan[i]==k[i]) diem+=(float)2/3;
            }
        }
        if(a==102){
          char h[102]={'A','C','C','A','B','C','D','D','B','B','C','D','D','B','B'};
            for(int i=0;i<15;i++){
                if(dapan[i]==h[i]) diem+=(float)2/3;
            }
        }
        cout<<fixed<<setprecision(2)<<diem<<endl;
        n--;
    }


}
CÂN BẰNG CHẴN LẺ
#include <iostream>
#include <cmath>

using namespace std;
int KTcbCL(int a){
    int csc=0,csl=0;
    while(a>0){
        int k;
        k=a%10;
        if(k%2==0) csc+=1;
        else csl+=1;
        a/=10;
        }
        if(csc==csl) return 1;
        else return 0;

}
int main (){
    int t;
    cin >> t;
    int dem=0;
    for(int i=pow(10,t-1);i<pow(10,t);i++){
        if(KTcbCL(i)==1) {
        cout<<i;dem+=1;
        if(dem==10) {cout<<endl; dem=0;}
        else cout<<" ";

    }}}
KHOẢNG CÁCH
#include <iostream>
#include <cmath>
#include<iomanip>
using namespace std;
int main (){
    int t;
    cin >> t;
    while(t>0){
    double a,b,c,d;
    cin>>a>>b>>c>>d;
    cout<<fixed<<setprecision(4)<<sqrt(pow(c-a,2)+pow(d-b,2))<<endl;
      t--;
    }

    }
    MÃ SỐ QUỐC GIA
    #include <iostream>
#include <cmath>
#include<string.h>
using namespace std;

int main (){
    int t;
    cin >>t;
    while(t>0){
    char x[100];
    cin>>x;
    int dem=0;
    for(int i=0;i<strlen(x);i++){
            if((int)x[i]-48==0 && (int)x[i+1]-48==8 && (int)x[i+2]-48==4){
                for(int m=0;m<i;m++){
                    cout<<x[m];
                }
                for(int n=i+3;n<strlen(x);n++){
                    cout<<x[n];
                }

            }
    }

    cout<<endl;

      t--;
    }

    }
 SỐ LIỀN KỀ
 #include <iostream>
#include <cmath>
#include<string.h>
using namespace std;

int main (){
    int t;
    cin >>t;
    while(t>0){
    char x[100];
    cin>>x;
    int dem=0;
    for(int i=0;i<strlen(x);i++){
        if(((int)x[i]-(int)x[i+1]==1)||((int)x[i]-(int)x[i+1]==-1)) dem+=1;
    }
    if(dem==strlen(x)-1) cout<<"YES\n";
    else cout<<"NO\n";

      t--;
    }

    }
    SỐ LẶP LẠI
    #include <iostream>
#include <cmath>
#include<string.h>
using namespace std;
int UCLN(int a,int b){
if(b!=0) return UCLN(b,a%b);
else return a;

}

int main (){
    int t;
    cin >>t;
    while(t>0){
    long long a,x,y;
    cin>>a>>x>>y;

    for(int i=1;i<=UCLN(x,y);i++){
        cout<<a;
    }
    cout<<endl;
    t--;
    }

    }
CHIA HẾT CHO BA SỐ NGUYÊN
#include <iostream>
#include <cmath>
#include<algorithm>// thư viện này để __gcd (tức là 1 hàm tìm ước chung lớn nhất trong thư viện này ở C++ , nó dùng phương pháp Eculis để tìm UCLN , mình chỉ sẵn dùng 
using namespace std;
int main (){
    long long t;
    cin >>t;
    while(t>0){
    long long x,y,z,n;
    cin>>x>>y>>z>>n;
    long long s=(x*y)/__gcd(x,y);
    long long BCNN3so=(s*z)/__gcd(s,z);//tìm bội chung nhỏ nhất của 3 số a,b,c
    if(BCNN3so>=pow(10,n)) cout<<-1<<endl;//nếu bội chung nhỏ nhất(tức là số nhỏ nhất chia hết cho 3 số a,b,c) mà lớn hơn cả số lớn nhất thỏa mãn để bài(ví dụ n=3 thì số lớn nhất thỏa mãn đề bài là 999 nhưng nếu BCNN mà lớn hơn hoặc bằng 1000 thì không tm) thì tức là không tồn tại và in ra -1
    else {//ngược lại nếu thỏa mãn
    double i= pow(10,n-1)/BCNN3so;// ta cần nhân BCNN với 1 số i sao cho  BCCN * i >= p=pow(10,n-1) (p là số nhỏ nhất thỏa mãn đề bài)  
    if((long long)i==i) BCNN3so=BCNN3so*i;//nếu i là số nguyên thì nhân luôn còn nếu không thì ta sẽ lấy phần nguyên +1 rồi nhân là sẽ ra kết quả bài toán
    else BCNN3so=BCNN3so*((long long)i+1);
    cout<<BCNN3so<<endl;}

    t--;
    }

    }
SỐ SPHENIC
#include <iostream>
#include <cmath>
using namespace std;
int KTsoF(int n){
int dem=0,dem1=0;
int i=2;
while(n%i==0){
    n/=i;
    dem1+=1;
    dem+=1;
}
if(dem1>1) return 0;
for(int i=3;i<=sqrt(n);i+=2){
        dem1=0;
    while(n%i==0){
        n/=i;
        dem1+=1;
        dem+=1;
    }
if(dem1>1) return 0;
}
if(n!=1) dem+=1;
if(dem==3) return 1;
else return 0;
}
int main (){
    int t;
    cin >>t;
    while(t>0){
    int k;
    cin>>k;
    cout<<KTsoF(k)<<endl;
    t--;
    }

    }
CHIA HẾT CHO A VÀ B
#include <iostream>
#include <math.h>
using namespace std;
int main (){
    int t;
    cin >>t;
    while(t>0){
    int m,n,a,b;
    cin>>m>>n>>a>>b;
    int dem=0;
    for(int i=m;i<=n;i++){
        if(i%a==0 || i%b==0) dem+=1;
    }
    cout<<dem<<endl;

    t--;
    }

    }
SỐ TĂNG GIẢM
#include <iostream>
#include <cmath>
using namespace std;
int KTsoNT(int a){
    if(a<2) return 0;
    else {
        for(int i=2;i<=sqrt(a);i++){
            if(a%i == 0 ) return 0;
        }
        return 1;
    }
}
int KTsoTG(int a){
    int a1=a;
    int sotrc=a%10;
    a=a/10;
    int tang=0,giam=0,scs=0;
    while(a>0){
        int sosau;
        sotrc=a%10;
        a/=10;
        sosau=a1%10;
        a1/=10;
        scs+=1;
        if(sotrc<sosau){
            tang+=1;
        }
        if(sotrc>sosau){
            giam+=1;
        }
        if(tang >0 && giam >0) return 0;
        }
        if(tang == scs || giam == scs) return 1;
        else return 0;
}
int tinhmu(int a){
    int kq=1;
   int i=0;
    while(i!=a){
        kq=kq*10;
        i+=1;
    }
    return kq;
}
 int main (){
    int t;
    cin >>t;
    while(t>0){
    int dem=0;
    int a;
    cin>>a;
    for(int i=tinhmu(a-1);i<tinhmu(a);i++){
        if(KTsoTG(i)==1 &&KTsoNT(i)==1)  dem+=1;
    }
    cout<<dem<<endl;
    t--;
    }

    }
ƯỚC SỐ CHUNG - BỘI SỐ CHUNG 
#include<bits/stdc++.h>
using namespace std;
int main()
{
    int k;
    long a,b;
    cin>>k;
    for(int i=1;i<=k;i++){
        cin>>a>>b;
        cout<<(a*b)/__gcd(a,b)<<" "<<__gcd(a,b)<<endl;
    }

}
ƯỚC SỐ CHUNG LỚN NHẤT CỦA N SỐ NGUYÊN DƯƠNG ĐẦU TIÊN
#include <iostream>

using namespace std;
int UCLN(long a,long b){
    if(b==0) return a;
    else return UCLN(b,a%b);
}
int main()
{
    long a;
   cin>>a;
   while(a>0){
    long k;
    cin>>k;
    long t=1;
    for(int i=1;i<=k;i++){
        t=(i*t)/UCLN(i,t);

    }
    cout<<t<<endl;
    a--;
   }
    return 0;
}
PHÂN TÍCH THỪA SỐ NGUYÊN TỐ - 1
#include<iostream>
#include<cmath>
using namespace std;
int main(){
    int n;
    cin>>n;
    while(n--){
    long long k;
    cin>>k;
    int i=2;
    int dem=0;
    while(k%i ==0){
      k/=i;
      dem+=1;
    }
    if(dem>0){
    cout<<i<<" "<<dem<<" ";
    }
    for(int i=3;i<=sqrt(k);i+=2){
            dem=0;
        if(k%i ==0){
            while(k%i ==0){
               k/=i;
               dem+=1;
            }
            cout<<i<<" "<<dem<<" ";
        }
    }
    if(k>1) cout<<k<<" 1 ";
        cout<<endl;
    }

    }
PHÂN TÍCH THỪA SỐ NGUYÊN TỐ - 2
#include<iostream>
#include<cmath>
using namespace std;
int main(){
    long long k;
    cin>>k;
    int i=2;
    int dem=0;
    while(k%i ==0){// đây là bước phân tích k thành pow(2,n)*m , với m là số lẻ (vì chỉ có số chẵn mới chia hết cho 2)
      k/=i;
      dem+=1;
    }
    if(dem>0){
    cout<<i<<" "<<dem<<"\n";
    }
    for(int i=3;i<=sqrt(k);i+=2){// đây là bước kiểm tra xem m có phải số nguyên tố hay không , sở dĩ i phải chạy từ 2 nhưng mà ở đây vì m là số lẻ(mà số lẻ chỉ chia hết cho số lẻ mà thôi) nên ta bắt đầu giá trị i từ i=3 và i+=2 để giá trị i tiếp theo là số lẻ
            dem=0;
        if(k%i ==0){
            while(k%i ==0){// nếu sau khi phân tích vòng lặp mà chạy chứng tỏ nó không phải số nguyên tố còn nếu không chạy thì nó là số nguyên tố hoặc k<i ..... , và ta được m là số nguyên tố thì vòng lặp ko chạy và sẽ in luôn , còn nếu m không phải số ngủyêm tố thì ta sẽ chia m cho số nguyên tố để thực hiện bước phân tích thành số nguyên tố và dĩ nhiên i lẻ là i đến căn k luôn cho ta điều kiện i là số nguyên tố 
               k/=i;
               dem+=1;
            }
            cout<<i<<" "<<dem<<"\n";
        }
    }
    if(k>1) cout<<k<<" 1\n";// có 2 trường hợp khiến vòng lặp không chạy đó là : 1. nếu k<i(TH k=8)  2.Nếu k là số nguyên tố nên ta mới cần điều kiện k>1 để thỏa mãn là số nguyên tố vì vòng lặp không còn chạy thì mới in ra

    }
NGUYÊN TỐ CÙNG NHAU
#include <iostream>
#include<cmath>
using namespace std;
int UCLN(int a,int b){
   if(b==0) return a;
   else return UCLN(b,a%b);
}
int KTsoNT(int a){
    if(a<2) return 0;
    else {
        for(int i=2;i<=sqrt(a);i++){
            if(a%i == 0 ) return 0;
        }
        return 1;
    }
}
int main(){
    int k;
    cin>>k;
    while(k--){
    int x;
    cin>>x;
    int t=0;
    for(int i=1;i<=x;i++){
        if(UCLN(i,x)==1) t+=1;
    }
    if(KTsoNT(t)==1) cout<<"1\n";
    else cout<<"0\n";

}
}
ƯỚC SỐ CHIA HẾT CHO 2
#include<iostream>
#include<cmath>
using namespace std;
int souocchan(long long n){
   int i=2;
   int dem=0;
   int socach=1;
   if(n%2 !=0) return 0;
   while(n%i==0){
    n/=i;
    dem+=1;
   }
   socach=dem;
   for(int i=3;i<=sqrt(n);i+=2){
        dem=0;
    while(n%i==0){
        n/=i;
        dem+=1;
    }
   socach*=dem+1;
   }
   if(n!=1) socach*=2;
   return socach;
}
    int main() {
    int k;
    cin>>k;
    while(k--){
    long long n;
    cin>>n;
    cout<<souocchan(n)<<endl;

}
    }
ƯỚC SỐ CỦA GIAI THỪA
#include<iostream>
#include<cmath>
using namespace std;
double tinhloga(long long a,long long b){
    return (double)log(b)/log(a);

}
    int main() {
    int k;
    cin>>k;
    while(k--){
    long long N,p;
    cin>>N>>p;
    long long h;
    int dem=0;
    if((double)tinhloga(p,N)==(long long)tinhloga(p,N)) h=tinhloga(p,N);
    else h=tinhloga(p,N)+1;

    for(int k=1;k<=h;k++){
        dem+=(long long)N/pow(p,k);
    }
    cout<<dem<<endl;}}
SỐ HOÀN HẢO
#include <iostream>

using namespace std;

int main()
{
    unsigned long long int n;
    int t;
    cin >> t;
    for (int i = 0; i < t; i++)
    {
        cin >> n;
        if (n == 6 || n == 28 || n == 496 || n == 8128 || n == 33550336 || n == 8589869056 || n == 137438691328)
            cout << "1" << endl;
        else
            cout << "0" << endl;
    }
}
ƯỚC SỐ NGUYÊN TỐ
#include <iostream>
#include <cmath>

using namespace std;

int main (){
    int t;
    cin >> t;
    while (t --){
        long long n;
        cin >> n;
        while (n % 2 == 0){
            cout << "2 ";
            n /= 2;
        }
        for (int i = 3; i <= sqrt(n); i += 2){
            while (n % i == 0){
                cout << i << " ";
                n = n / i;
            }
        }
        if (n > 2)
            cout << n;
        cout << "\n";
    }
}
ƯỚC SỐ NGUYÊN TỐ LỚN NHẤT
#include<iostream>
#include<cmath>
using namespace std;
    int main() {
    int k;
    cin>>k;
    while(k--){
    long long n;
    cin>>n;
    long long i=2;
    long long dem=0;
    while(n%i == 0){
        n/=i;
        dem+=1;
    }
    long long l,dem1=0;
    for(l=3;l<=sqrt(n);l+=2){
        while(n%l ==0){
            n/=l;
            dem1+=1;
        }

    }
    if(n!=1) cout<<n<<endl;
    if(n==1 && dem1==0) cout<<i<<endl;
    if(n==1 && dem1>0) cout<<l-2<<endl;
    }
    }
