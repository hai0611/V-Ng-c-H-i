    PHẦN TỬ NHỎ NHẤT THỨ K
    #include <iostream>
using namespace std;
int main (){
    int t;
    cin >>t;
    long x[1000];
    long n,k;
    while(t>0){
    cin>>n>>k;
    for(int i=1;i<=n;i++){
        cin>>x[i];
    }
    for(int i=1;i<=n;i++){
        for(int j=1+i;j<=n;j++){
            int k;
            if(x[i]>x[j]){
                k=x[i];
                x[i]=x[j];
                x[j]=k;
            }
        }
    }

        cout<<x[k]<<endl;


    t--;
    }

    }
