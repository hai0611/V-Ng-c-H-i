int KTsoNT(int a){
    if(a<2) return 0;
    else{
        for(int i=2;i<=sqrt(a);i++){
        if(a%i==0) return 0 ;

        }
        return 1;
    }
}
void setup(int k){
    int dem=0;
    int i=2;
    if(KTsoNT(k-i)==1) {cout<<i<<" "<<k-i;dem+=1;}
    if(dem==0){
    for(i=3;i<=k/2;i+=2){
        if(KTsoNT(i)==1 && KTsoNT(k-i)==1) {cout<<i<<" "<<k-i;dem+=1;break;}
    }}
    if(dem==0) cout<<-1;

    }

int main()
{
    int a;
    cin>>a;
    while(a>0){
    int k;
    cin>>k;
    setup(k);
    cout<<endl;
    a--;
   }
}
