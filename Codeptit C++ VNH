        }
        return 1;
    }
}
void setup(int k){//tại sao kiểm tra số nguyên tố lại chạy đến căn n
for(int i=1;i<=k;i++){
    if(i==1) cout<<1<<" ";
    if(i% 2 == 0 && i>2) cout<<2<<" ";
    if(KTsoNT(i)==1) cout<<i<<" ";
    if(KTsoNT(i)!=1 && i>3 && i%2!=0){
        int dem=0;
        int j=3;
        int m=i;
        while(dem!=1){
            if(i%j ==0){
                cout<<j<<" ";
                dem+=1;}
            j+=2;}
            }
            }}
