#include<bits/stdc++.h>
using namespace std;
long long convertkitusangso(string s){
long long k=0,h=1;
for(int i=0;i<s.size();i++){
    k+=(s[s.size()-i-1]-'0')*h;
    h*=10;
}
return k;
}
string summin(string s){
    for(int i=0;i<s.size();i++){
        if(s[i]=='6') s[i]='5';
    }
    return s;
}
string summax(string s){
    for(int i=0;i<s.size();i++){
        if(s[i]=='5') s[i]='6';
    }
    return s;
}
int main(){
    int t=0;
    cin>>t;
    cin.ignore();
    while(t--){
    string s1,s2;
    cin>>s1>>s2;
    cout<<convertkitusangso(summin(s1))+convertkitusangso(summin(s2))<<" "<<convertkitusangso(summax(s1))+convertkitusangso(summax(s2))<<endl;

}}
TÍNH TỔNG CÁC SỐ TRONG XÂU
#include<bits/stdc++.h> 
using namespace std; 
int main() { 
    int t; 
    cin>>t; 
    while(t--) { 
        string str; 
        cin>>str; str = 'a' + str; 
        stack<char> s; 
        long long sum = 0; 
        for(int i=str.size()-1; i>=0; i--) { 
            if(str[i]>='a' && str[i]<='z') { 
                bool check = false; int res = 0; 
                while(!s.empty()) { 
                    check = true; int x = s.top()-'0'; res = res*10 + x; s.pop(); } 
                    if(check) { sum += res; } } 
                    else { s.push(str[i]); } } 
                    cout<<sum<<endl; } }
 TÌM SỐ LỚN NHẤT TRONG XÂU
 #include<bits/stdc++.h> 
using namespace std; 
int main() { 
    int t; 
    cin>>t; 
    while(t--) { 
        string str; 
    cin>>str; str = 'a' + str; 
    stack<char> s; 
    long long maxRes = 0; 
    for(int i=str.size()-1; i>=0; i--) { 
        if(str[i]>='a' && str[i]<='z') { 
            if(!s.empty()) { long long res = 0; 
    while(!s.empty()) { long long x = s.top()-'0'; res = res*10 + x; s.pop(); } maxRes = max(maxRes, res); } } 
    else { s.push(str[i]); } } 
    cout<<maxRes<<endl; } }
ĐẾM XÂU CON
#include<bits/stdc++.h>
using namespace std;
int countsubs(string s,int k){
    int n=s.size();
    int numbersub=0;
    for(int i=0;i<n;i++){
        int numberdiff=0;
        vector<int>h(26,0);
        for(int j=i;j<n;j++){
        if(h[s[j]- 'a']==0) numberdiff+=1;
        h[s[j]- 'a']++;
        if(numberdiff == k) numbersub+=1;
        if(numberdiff > k) {break;}
        }
    }
    return numbersub;
}
int main(){
    int t=0;
    cin>>t;
    cin.ignore();
    while(t--){
    string s;
    int k;
    cin>>s>>k;
    cout<<countsubs(s,k)<<endl;
}}
TÁCH SỐ VÀ TÍNH TỔNG
#include<bits/stdc++.h>
using namespace std;
int main(){
    int t=0;
    cin>>t;
    while(t--){
    string s;
    cin>>s;
    int n=s.size();
    char chu[100000];
    char so[100000];
    bool check[100000]={0};
    for(int i=0;i<n;i++){
        if(s[i]>='A' && s[i] <= 'Z') check[i]=1;
    }
    int index_1=0,index_2=0;
    for(int i=0;i<n;i++){
        if(check[i]==1) {chu[index_1]=s[i];index_1+=1;}
        else {so[index_2]=s[i];index_2+=1;}
    }
    int tong=0;
    for(int i=0;i<index_2;i++){
        tong+=(so[i]-'0');
    }
    sort(chu,chu+index_1);
    for(int i=0;i<index_1;i++){
        cout<<chu[i];
    }
    cout<<tong<<endl;
}}
 BIỂN SỐ ĐẸP
 #include<bits/stdc++.h>
using namespace std;
int th1(string s){
for(int i=0;i<4;i++){
    if(s[i]>=s[i+1]) return 0;
}
return 1;
}
int th2(string s){
for(int i=0;i<4;i++){
    if(s[i]!= s[i+1]) return 0;
}
return 1;
}
int th3(string s){
for(int i=0;i<4;i++){
    if(s[i]!='6' && s[i] !='8') return 0;
}
return 1;
}
int th4(string s){
for(int i=0;i<2;i++){
    if(s[i]!=s[i+1]) return 0;
}
if(s[3]!=s[4]) return 0;
return 1;
}
int main(){
    int t=0;
    cin>>t;
    while(t--){
    string s;
    cin>>s;
    int n=s.size();
    string daucham = ".";
    int k=s.find(daucham);
    s.replace(k,1,"");
    string work=s.substr(5,5);
    if(th1(work)==1 || th2(work)==1 || th3(work)==1 || th4(work)==1 ) cout<<"YES\n";
    else cout<<"NO\n";

}}
SỐ ĐẦY ĐỦ
#include<bits/stdc++.h>
using namespace std;
string check(string s){
int n=s.size();
if(s[0]-'0'==0) return "INVALID";
for(int i=0;i<n;i++){
        if((s[i]-'0')<0 || (s[i]-'0')>9) return "INVALID";
    }
int x[100];
x[1]=s.find('1');
x[2]=s.find('2');
x[3]=s.find('3');
x[4]=s.find('4');
x[5]=s.find('5');
x[6]=s.find('6');
x[7]=s.find('7');
x[8]=s.find('8');
x[9]=s.find('9');
x[0]=s.find('0');
for(int i=0;i<10;i++){
    if(x[i]==-1) return "NO";
}
return "YES";
}
int main(){
    int t=0;
    cin>>t;
    while(t--){
    string s;
    cin>>s;
    cout<<check(s)<<endl;
}}
ĐIỆN THOẠI CỤC GẠCH
#include<bits/stdc++.h>
using namespace std;
int checktn(vector<int>x){
    int n=x.size();
    for(int i=0;i<n/2;i++){
        if(x[i] != x[n-1-i]) return 0;
    }
    return 1;
}
int main()
{
    int t=0;
    cin>>t;
    while(t--){
        vector<int>dayso;
        string s;
        cin>>s;
        string s1[11]={ "0","1","ABC","DEF","GHI","JKL","MNO","PQRS","TUV","WXYZ"};
        for(int j=0;j<s.size();j++){
        for(int i=2;i<=9;i++){
            if(islower(s[j])) s[j]=toupper(s[j]);
            int k=s1[i].find(s[j]);
            if(k!=-1) dayso.push_back(i);
        }
        }
    if(checktn(dayso)==1) cout<<"YES\n";
    else cout<<"NO\n";
    }
}
CHIA HẾT CHO 11
#include <bits/stdc++.h> 
using namespace std; 
bool check(string str){ 
    int dodai = str.length(); 
    int sumchan = 0,sumle = 0; 
    for(int i = 0;i<dodai;i++){ 
            if(i%2==0){ sumchan+=(str[i]-'0'); }
    else{ sumle += (str[i]-'0'); } } 
    int hieu = sumle - sumchan; 
    if(hieu%11==0){ return true; }
    else{ return false; } } 
    int main(){ int t; 
    cin>>t; 
    while(t--){ 
        string str; 
    cin>>str; 
    if(check(str)){ cout<<1<<endl; }
    else{ cout<<0<<endl; } } return 0; }
DÃY ƯU THẾ
#include<bits/stdc++.h>
using namespace std;
int main()
{
   int t=0;
   cin>>t;
   while(t--){
    int a;
    char kitu = ' ';
    int csc =0 , csl=0;
    while(kitu != '\n'){
        cin>>a;
        if(a % 2 == 0) csc +=1;
        else csl+=1;
        kitu = getchar();
    }
    int spt = csc + csl;
  if( spt % 2 == 0 && csc > csl) cout<<"YES\n";
  else if( spt % 2 != 0 && csl > csc) cout<<"YES\n";
  else cout<<"NO\n";
   }
}
