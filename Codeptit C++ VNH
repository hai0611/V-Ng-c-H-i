KIỂM TRA SỐ FIBONACCI
#include <iostream>
#include<cmath>
using namespace std;//số Fibonaci bắt đầu từ 0 nhé , 0,1,2 đều là số Fibonaci
int main()
{
    int a;
    cin>>a;
    while(a--){
    long long k;
    cin>>k;
    if(k==0) {cout<<"YES\n";continue;}
    long long F[100000];
    F[1]=1;
    F[2]=1;
    long long i=3,h=0;
    while(h<=k){
        F[i]=F[i-1]+F[i-2];
        h=F[i];
    i++;
    }
    if(F[i-2]==k) cout<<"YES\n";
    else cout<<"NO\n";

   }}
  KIỂM TRA DÃY FIBONACCI
 #include <iostream>
#include<cmath>
using namespace std;
int isFibo(int k){
   if(k==0) return 1;
    long long F[100000];
    F[1]=1;
    F[2]=1;
    int i=3,h=0;
    while(h<=k){
        F[i]=F[i-1]+F[i-2];
        h=F[i];
    i++;
    }
    if(F[i-2]==k) return 1;
    else return 0;
}
