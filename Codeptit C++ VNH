using namespace std;
int main() {
        int n;
        cin>>n;
        int a[1002][5];
        int dem=0;
        for(int i=1;i<=n;i++){
            for(int j=1;j<=3;j++){
                cin>>a[i][j];
            }
        }
        for(int i=1;i<=n;i++){
            int dem1=0,dem0=0;
        for(int j=1;j<=3;j++){
                if(a[i][j]==1) dem1+=1;
                else dem0+=1;
                    }
        if(dem1>dem0) dem+=1;
                    }
 cout<<dem<<endl;
}
TÍCH MA TRẬN
#include<bits/stdc++.h>
using namespace std;
void nhapMaTran(int a[][100],int row,int column) {
for(int i=1;i<=row;i++){
            for(int j=1;j<=column;j++){
                cin>>a[i][j];
            }
        }
}
void xuatMaTran(int a[][100],int row,int column) {
for(int i=1;i<=row;i++){
            for(int j=1;j<=column;j++){
                cout<<a[i][j]<<" ";
            }
            cout<<endl;
        }
}
void tichHaiMaTran(int a[][100],int b[][100],int c[][100],int m,int n,int p){
for(int i=1;i<=m;i++){
    for(int j=1;j<=p;j++){
        for(int l=1;l<=n;l++){
            c[i][j]+=a[i][l]*b[l][j];
        }
    }
}
}
int main() {
        int n,m,p;
        cin>>n>>m>>p;
        int a[100][100];
        int b[100][100];
        int MaTranAB[100][100];
        nhapMaTran(a,n,m);
        nhapMaTran(b,m,p);
        tichHaiMaTran(a,b,MaTranAB,n,m,p);
        xuatMaTran(MaTranAB,n,p);
}
