CẶP SỐ NGUYÊN TỐ
#include<iostream>
#include<cmath>
using namespace std;
int KTsoNT(int n){
if(n<2) return 0;
else {
    for(int i=2;i<=sqrt(n);i++){
        if(n%i ==0 ) return 0;
    }
    return 1;
}}
    int main() {
    int k;
    cin>>k;
    while(k--){
    int n;
    cin>>n;
    for(int i=2;i<n;i++){
        if(KTsoNT(i)==1 && KTsoNT(n-i)==1) {cout<<i<<" "<<n-i;break;}
    }
   cout<<endl;
    }
    }
ƯỚC SỐ NGUYÊN TỐ THỨ K
#include<iostream>
#include<cmath>
using namespace std;
int KT(int N,int k){
        int i=2;
        int dem=0;
        while(N%i == 0){
            N/=i;
            dem+=1;
            if(dem==k) return 2;
        }


        for(i=3;i<=sqrt(N);i+=2){
            while(N%i==0){
            N/=i;
            dem+=1;
            if(dem==k) {return i;}}}
        if(N!=1) {
            dem+=1;
        if(dem==k) {return N;}
        else return -1;}
        else return -1;
        }
    int main() {
    int n;
    cin>>n;
    while(n--){
        int N,k;
        cin>>N>>k;
        cout<<KT(N,k)<<endl;
            }
        }
