 #include <bits/stdc++.h>
using namespace std;
int main(){
    int t=0;
    cin>>t;
    while(t--){
        cin.ignore();
        string ch1;
        getline(cin,ch1);
        int k;
        cin>>k;
        //cin.ignore();
        if(ch1.length()<26) { cout<<0<<endl;continue;}
        else{
        int check[100000]={0},dem=0;
        for(int i='a';i<='z';i++){
            for(int j=0;j<ch1.length();j++){
                if(ch1[j]==i) check[i]=1;
            }
        }
        for(int i='a';i<='z';i++){
            if(check[i]==0) dem+=1;
        }
        if(dem<=k) cout<<1<<endl;
        else cout<<0<<endl;
        }
    }
}
