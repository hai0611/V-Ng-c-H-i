TÍNH GIÁ TRỊ ĐA THỨC
#include <iostream>
#include<cmath>
#include<algorithm>
using namespace std;
#define f1 1000000007
int main()
{
 int SLboTest;
 cin>>SLboTest;
 while(SLboTest--){
 long long n,x;
 cin>>n>>x;
 long long a[2000];
 for(long long i=0;i<n;i++){
    cin>>a[i];
 }
 reverse(a,a+n);
 long long total=0;
 for(long long i=n-1;i>=0;i--){
    long long xmu=1;
        for(int j=1;j<=i;j++){
            xmu=(xmu*x)%f1;
        }
    total+=a[i]*xmu;
 }
 if (total > f1) { total %= f1; }
    cout << total << endl;
}}
HIỆU LỚN NHẤT CỦA CẶP PHẦN TỬ ĐÚNG THỨ TỰ
#include <bits/stdc++.h>

using namespace std;

int main()
{
  int SLboTest;
  cin>>SLboTest;
  while(SLboTest--){
  int n;
  cin>>n;
  int x[100000];
  for(int i=1;i<=n;i++){
    cin>>x[i];
  }
int HieuLonNhatCua2PhanTu,dem=0;
for(int i=1;i<=n;i++){
    for(int j=i+1;j<=n;j++){
     if(x[i]<x[j]){
        HieuLonNhatCua2PhanTu = x[j]-x[i];
        dem+=1;
        break;
     }
    }
}
if(dem==0) cout<<-1<<endl;
else{
for(int i=1;i<=n;i++){
    for(int j=i+1;j<=n;j++){
     if(x[i]<x[j]){
        if(x[j]-x[i]>HieuLonNhatCua2PhanTu){
            HieuLonNhatCua2PhanTu = x[j]-x[i];
        }
     }
    }
}
cout<<HieuLonNhatCua2PhanTu<<endl;
}}}
TỔNG LỚN NHẤT
#include<bits/stdc++.h>
using namespace std;

void NhapMang(int k[],int soPhanTuMang){
    for(int i=1;i<=soPhanTuMang;i++){
        cin>>k[i];
    }
}

int tongCuaCacSoycbt(int x[],int n,int y[],int m) {
    int sum=0;
    int k=0;
for(int i=1;i<=n;i++){
    k+=x[i];
    int h=0;
    for(int j=i+1;j<=m;j++){
        h+=y[j];
    }
    if(k+h > sum) sum = h+k;
}
return sum;
}

int main() {
    int SLboTest;
    cin >> SLboTest;
    while(SLboTest--) {
        int n,m;
        int x[10000],y[10000];
        cin>>n>>m;
        NhapMang(x,n);
        NhapMang(y,m);
        int tongCacChuSoStartX = tongCuaCacSoycbt(x,n,y,m);
        int tongCacChuSoStartY = tongCuaCacSoycbt(y,m,x,n);
        if(tongCacChuSoStartX > tongCacChuSoStartY) cout<<tongCacChuSoStartX<<endl;
        else cout<<tongCacChuSoStartY<<endl;
        }}
DÃY MOUNTAIN
#include<bits/stdc++.h>
using namespace std;

void nhapMang(int k[],int soPhanTuMang){
    for(int i=0;i<soPhanTuMang;i++){
        cin>>k[i];
    }
}
int viTriSoMaxTrongKhoangLR(int a[],int l,int r){
    int k = a[l];
    int j;
    for(int i=l;i<=r;i++){
        if(a[i]>k) {
            k=a[i];
            j=i;
        }
    }
    if(k==a[l]) return l;
    return j;
}

int kiemTraTang (int a[], int l, int maxx) {
for(int i = l;i<maxx;i++){
   if(a[i] > a[i+1]) return 0;
    }
    return 1;

}

int kiemTraGiam(int x[],int maxx,int r) {
for(int i = maxx;i>r;i--){
    if(x[i] < x[i+1]) return 0;
    }
    return 1;
}

int main() {
    int SLboTest;
    cin >> SLboTest;
    while(SLboTest--) {
        int n,a[100000];
        int dem=0;
        cin>>n;
        nhapMang(a,n);
        int l,r;
        cin>>l>>r;
        int maxx=viTriSoMaxTrongKhoangLR(a,l,r);
        if(kiemTraTang(a,l,r)==1) {cout<<"Yes\n";dem+=1;}
        if(kiemTraTang(a,l,r)==1 && dem==0) {cout<<"Yes\n";dem+=1;}
        if(a[l]<=a[l+1] && dem==0){
        if(kiemTraTang(a,l,maxx)==1 && kiemTraGiam(a,maxx,r)==1) {cout<<"Yes\n";dem+=1;}
}
        if(dem==0) cout<<"No\n";

}}
 ĐẾM SỐ NGUYÊN TỐ
 #include<bits/stdc++.h>
using namespace std;
int kiemTraSoNT(int a){
if(a<2) return 0;
else{
        for(int i=2;i<=sqrt(a);i++){
            if(a%i==0) return 0;
        }
        return 1;
}
}
int main() {
    int SLboTest;
    cin >> SLboTest;
    while(SLboTest--) {
        int l,r;
        int dem=0;
        cin>>l>>r;
        for(int i=l;i<=r;i++){
            if(kiemTraSoNT(i)==1) dem+=1;
        }
        cout<<dem<<endl;

}}
BIẾN ĐỔI DÃY SỐ

#include<bits/stdc++.h>
using namespace std;
int main() {
    int SLboTest;
    cin >> SLboTest;
    while(SLboTest--) {
        int dem=0;
        int n;
        cin>>n;
        int a[n+1];
        for(int i=1;i<=n;i++){
           cin>>a[i];
        }
        int left=1,right=n;
        while(left<=right) {
        if(a[left]== a[right]){
            left++;
            right--;
        }
        else if(a[left]>a[right]){
            right--;
            a[right]+=a[right+1];
            dem+=1;
        }
        else {
            left++;
            a[left]+=a[left-1];
            dem+=1;
        }
        }
        cout<<dem<<endl;
}}
