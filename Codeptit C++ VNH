KHOẢNG CÁCH NHỎ NHẤT

#include <iostream>

using namespace std;

int main()
{
 int SLboTest;
 cin>>SLboTest;
 while(SLboTest--){
 int n;
 cin>>n;
 int A[10000];
 for(int i=1;i<=n;i++){
    cin>>A[i];
 }
 for(int i=1;i<=n;i++){
    for(int j=1+i;j<=n;j++){
      if(A[i]>A[j]) {
        int k;
        k=A[i];
        A[i]=A[j];
        A[j]=k;
      }
    }
 }
 int khoangCachNhoNhat=A[2]-A[1] ;
for(int i=1;i<=n;i++){
    for(int j=1+i;j<=n;j++){
            if(A[j]-A[i]<khoangCachNhoNhat) {
                khoangCachNhoNhat=A[j]-A[i];
            }
 }}
cout<<khoangCachNhoNhat<<endl;

}}
CHÊNH LỆCH NHỎ NHẤT
#include <bits/stdc++.h>
using namespace std;
int main() {
    int t;
    cin >> t;
    while(t--) {
        int n;
        cin >> n;
        int a[n];
        int i;
        for(i = 0; i<n; i++) { cin >> a[i]; }
        sort(a,a+n);
        int tmp = a[n-1] - a[n-2];
        for(i = n-2; i>0; i--) { if((a[i]-a[i-1]) < tmp) tmp = a[i] - a[i-1]; }
        cout << tmp << endl; } }
