    while(SLboTest--) {
        int n,m;
        cin>>n>>m;
        int a[100][100];
        static int checkrow[1000];
        static int checkcolumn[1000];
        for(int i=1;i<=n;i++){
            for(int j=1;j<=m;j++){
                cin>>a[i][j];
                if(a[i][j]==1) {checkrow[i]=1;checkcolumn[j]=1; 
                }
            }
        }
        for(int i=1;i<=n;i++){
        for(int j=1;j<=m;j++){
                    if(checkrow[i]==1 || checkcolumn[j]==1){
                            cout<<"1 ";}
                    else{
                cout<<"0 ";}}
                cout<<endl;
}}}
