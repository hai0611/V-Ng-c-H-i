 #include<bits/stdc++.h>
using namespace std;
int main(){
int t;
cin>>t;
while(t--){
int n;
cin>>n;
int x[100000];
bool check[100000];
int dem=0;
for(int i=1;i<=n;i++) {cin>>x[i]; check[i]=1;}
for(int i=1;i<=n;i++) {if(check[i]==1){
    for(int j=1+i;j<=n;j++) {
        if(x[i]==x[j]) {
            check[i]=0;
            check[j]=0;
        }}}}
