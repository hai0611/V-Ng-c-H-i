#include<bits/stdc++.h>
using namespace std;
int main(){
    int t;
    cin>>t;
    cin.ignore();
    string s[100000];
    for(int i=1;i<=t;i++){
    getline(cin,s[i]);
    }
    bool check[100000];
    for(int i=1;i<=t;i++){
    for(int j=i+1;j<=t;j++){
        if(s[i].compare(s[j])==0) check[j]=1;
    }}
    int dem=0;
    for(int i=1;i<=t;i++){
        if(check[i]==0) dem+=1;
    }
    cout<<dem<<endl;
    }
 MÃ HÓA
 #include<bits/stdc++.h>
using namespace std;
int main()
{
    int t=0;
    cin>>t;
    while(t--){
    string s;
    cin>>s;
    bool check[100000]={0};
    int n=s.size();
    vector<int>count(26,0);
    for(auto ch:s)
        count[ch-'A']++;
    for(int i=0;i<n;i++){
        for(int j=i+1;j<n;j++){
            if(s[j]==s[i]) check[j]=1;
        }
    }
    for(int i=0;i<n;i++)
        if(check[i]==0) cout<<s[i]<<count[s[i]-'A'];
cout<<endl;
}}
LOẠI BỎ NGUYÊN ÂM
#include<bits/stdc++.h>
using namespace std;
int main(){
    string s;
    getline(cin,s);
    for(int i=0;i<s.size();){
        if(s[i]=='A' || s[i]=='E' || s[i]=='I' || s[i]=='O' || s[i]=='U' || s[i]=='Y' || s[i]=='a'|| s[i]=='e' || s[i]=='o' || s[i]=='u' || s[i]=='y'|| s[i]=='i'){
            s.replace(i,1,"");
        }
        else i++;
    }
    for(int i=0;i<s.size();i++){
        if(isupper(s[i])) cout<<"."<<(char)tolower(s[i]);
        if(islower(s[i])) cout<<"."<<s[i];
    }
}
 ĐỔI CHỖ CHỮ SỐ
 #include<bits/stdc++.h>
using namespace std;
string countcase(string s){
int n=s.size();
int index=-1,maxx=-1;
for(int i=n-2;i>=0;i--){
    if(s[i]>s[i+1]) {index=i;break;}
}
if(index==-1) return "-1";
for(int i=n-1;i>index;i--){
    if(s[i]<s[index]) {
        if(maxx==-1) maxx=i;
        else if(s[i]>=s[maxx]) maxx=i;
    }
}
if(maxx != -1) {swap(s[index],s[maxx]);return s;}
return "-1";
}
int main(){
    int t=0;
    cin>>t;
    cin.ignore();
    while(t--){
    string s;
    getline(cin,s);
    cout<<countcase(s)<<endl;
}}
 SẮP ĐẶT XÂU KÝ TỰ - 1
 #include<bits/stdc++.h>
using namespace std;
int main()
{
    int t=0;
    cin>>t;
    while(t--){
    string s;
    cin>>s;
    int n=s.size();
    vector<int>count(26,0);
    for(auto ch:s)
        count[ch-'a']++;
    int maxCount=0;
    for(int i=0;i<26;i++)
        if(count[i]>maxCount) maxCount=count[i];
    if(maxCount > (n+1)/2) cout<<0<<endl;
    else cout<<1<<endl;
    }
}
