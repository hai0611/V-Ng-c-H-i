    #include <bits/stdc++.h>
using namespace std;
void nhapMaTran2Chieu(int a[105][105],int n,int m){
    for(int i=1;i<=n;i++){
        for(int j=1;j<=m;j++){
            cin>>a[i][j];
        }
    }
}
void xuatMaTran2Chieu(int a[105][105],int n,int m){
    for(int i=1;i<=n;i++){
        for(int j=1;j<=m;j++){
            cout<<a[i][j]<<" ";
        }
        cout<<endl;
    }
}
void timDao(int a[105][105],int i,int j){
a[i][j]=0;
int toadoX[9]={0,-1,-1,-1,0,0,1,1,1};
int toadoY[9]={0,-1,0,1,-1,1,-1,0,1};
for(int k=1;k<=8;k++){
    if(a[i+toadoX[k]][j+toadoY[k]]==1) timDao(a,i+toadoX[k],j+toadoY[k]);
}
}
int main(){
  int SLboT;
  cin>>SLboT;
  while(SLboT--){
    int n,m;
    cin>>n>>m;
    int a[105][105];
    int soDao=0;
    nhapMaTran2Chieu(a,n,m);
    for(int i=1;i<=n;i++){
        for(int j=1;j<=m;j++){
            if(a[i][j]==1) {timDao(a,i,j);soDao+=1;}
        }
    }
    cout<<soDao<<endl;
}
}
