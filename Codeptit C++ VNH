QUAY MA TRẬN
#include <bits/stdc++.h>
using namespace std;
int main()
{
    int SLboTest;
    cin>>SLboTest;
    while(SLboTest--){
    int n,m;
    cin>>n>>m;
    int a[n+1][m+1];
    for(int i=1;i<=n;i++){
        for(int j=1;j<=m;j++){
            cin>>a[i][j];
        }
    }
    int hangDauTien=1,hangCuoiCung=n,cotDauTien=1,cotCuoiCung=m;
    while(hangDauTien<hangCuoiCung && cotDauTien<cotCuoiCung){
        int pre = a[hangDauTien+1][cotDauTien];
        for(int i=cotDauTien;i<=cotCuoiCung;i++){
            int cur = a[hangDauTien][i];
            a[hangDauTien][i]=pre;
            pre=cur;
        }
        hangDauTien+=1;
        for(int i=hangDauTien;i<=hangCuoiCung;i++){
            int cur = a[i][cotCuoiCung];
            a[i][cotCuoiCung]=pre;
            pre=cur;
        }
        cotCuoiCung-=1;
        for(int i=cotCuoiCung;i>=cotDauTien;i--){
            int cur = a[hangCuoiCung][i];
            a[hangCuoiCung][i]=pre;
            pre=cur;
        }
        hangCuoiCung-=1;
        for(int i=hangCuoiCung;i>=hangDauTien;i--){
            int cur = a[i][cotDauTien];
            a[i][cotDauTien]=pre;
            pre=cur;
        }
        cotDauTien+=1;
        }
    for(int i=1;i<=n;i++){
        for(int j=1;j<=m;j++){
            cout<<a[i][j]<<" ";
        }
    }
    cout<<endl;
    }
}
ĐẾM PHẦN TỬ GIỐNG NHAU
#include <bits/stdc++.h>
using namespace std;
int main()
{
    int SLboTest;
    cin>>SLboTest;
    while(SLboTest--){
    int n;
    cin>>n;
    int a[n+1][n+1];
    int b[100000];
    int dem=0;
    for(int i=1;i<=n;i++){
        for(int j=1;j<=n;j++){
            cin>>a[i][j];
        }
    }
    for(int i=1;i<=n;i++){
        b[a[1][i]]=1;
    }
    for(int i=2;i<=n;i++){
        for(int j=1;j<=n;j++){
            if(b[a[i][j]]==i-1){
                b[a[i][j]]+=1;
            }
        }
    }
    bool check[1][n+1];
    for(int j=1;j<=n;j++){
        check[1][j]=1;
    }
    for(int j=1;j<=n;j++){if(check[1][j]==1){
        for(int i=j+1;i<=n;i++){
            if(a[1][i]==a[1][j]) check[1][i]=0;}
    }}
    for(int j=1;j<=n;j++){if(check[1][j]==1){
       if(b[a[1][j]]==n) dem+=1;
    }}
        cout<<dem<<endl;
    }
}
