 SỐ SMITH
 #include <iostream>
#include<cmath>
using namespace std;
int KTsoNT(int a){
    if(a<2) return 0;
    else{
        for(long long i=2;i<=sqrt(a);i++){
        if(a%i==0) return 0 ;

        }
        return 1;
    }
}
int tongcacchusocua(int N){
    int tong=0;
    while(N>0){
        int k;
        k=N%10;
        N/=10;
        tong+=k;
    }
    return tong;
}

int main()
{
    int a;
    cin>>a;
    while(a>0){
    int k;
    cin>>k;
    int N=k;
    int i=2,dem=0,tong=0;
    while(k%2 ==0){
        k/=2;
        tong+=tongcacchusocua(2);
    }
    for(int i=3;i<=sqrt(k);i+=2){
        while(k%i ==0){
            k/=i;
            tong+=tongcacchusocua(i);
        }
    }
    if(k!=1) {tong+=tongcacchusocua(k);}
    if(KTsoNT(N)==0 && tong==tongcacchusocua(N)) cout<<"YES"<<endl;
    else cout<<"NO"<<endl;
    a--;
   }
}
