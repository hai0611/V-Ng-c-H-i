    int x[100000];
    for(int i=1;i<=n*n;i++){
            cin>>x[i];
        }
    sort(x+1,x+1+n*n);
    int hangDauTien=1,hangCuoiCung=n,cotDauTien=1,cotCuoiCung=n;
    int k=1;
    while(hangDauTien<=hangCuoiCung && cotDauTien<=cotCuoiCung){
        for(int i=cotDauTien;i<=cotCuoiCung;i++){
            a[hangDauTien][i]=x[k];
            k+=1;
        }
        hangDauTien+=1;
        for(int i=hangDauTien;i<=hangCuoiCung;i++){
            a[i][cotCuoiCung]=x[k];
            k+=1;
            }
        cotCuoiCung-=1;
        if(hangDauTien<=hangCuoiCung){
        for(int i=cotCuoiCung;i>=cotDauTien;i--){
            a[hangCuoiCung][i]=x[k];
            k+=1;}
        hangCuoiCung-=1;}
        if(cotDauTien<=cotCuoiCung){
        for(int i=hangCuoiCung;i>=hangDauTien;i--){
            a[i][cotDauTien]=x[k];
            k+=1;}
        cotDauTien+=1;
        }}
        for(int i=1;i<=n;i++){
            for(int j=1;j<=n;j++){
                cout<<a[i][j]<<" ";
            }
            cout<<endl;
        }
    }
