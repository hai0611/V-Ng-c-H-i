#include <bits/stdc++.h>
using namespace std;
void nhapMaTran2Chieu(int a[][105],int n,int m){
    for(int i=1;i<=n;i++){
        for(int j=1;j<=m;j++){
            cin>>a[i][j];
        }
    }
}
void xuatMaTran2Chieu(int a[][105],int n,int m){
    for(int i=1;i<=n;i++){
        for(int j=1;j<=m;j++){
            cout<<a[i][j]<<" ";
        }
        cout<<endl;
    }
}
int soPhepBienDoiMin(int a[][105],int n){
    int dem=0;
    int maxSum=0,sumRow[100000]={},sumCol[100000]={};
    for(int i=1;i<=n;i++){
        for(int j=1;j<=n;j++){
            sumRow[i]+=a[i][j];
            sumCol[j]+=a[i][j];
        }
    }
    for(int i=1;i<=n;i++){
        maxSum=max(sumRow[i],maxSum);
        maxSum=max(sumCol[i],maxSum);
    }
    int valueTangMin=0;
    for(int i=1,j=1;i<=n && j<=n;){
        valueTangMin=min(maxSum-sumRow[i],maxSum-sumCol[j]);
        sumCol[j]+=valueTangMin;
        sumRow[i]+=valueTangMin;
        dem+=valueTangMin;
        if(sumCol[j]==maxSum) j++;
        if(sumRow[i]==maxSum) i++;
    }
    return dem;
}
int main()
{
    int t;
    cin>>t;
    while(t--){
    int n;
    cin>>n;
    int a[105][105];
    nhapMaTran2Chieu(a,n,n);
    cout<<soPhepBienDoiMin(a,n)<<endl;
    }
}
HÌNH CHỮ NHẬT LỚN NHẤT - 1
#include <bits/stdc++.h>
using namespace std;
void nhapMaTran2Chieu(int a[][230],int n,int m){
    for(int i=1;i<=n;i++){
        for(int j=1;j<=m;j++){
            cin>>a[i][j];
        }
    }
}
void xuatMaTran2Chieu(int a[][230],int n,int m){
    for(int i=1;i<=n;i++){
        for(int j=1;j<=m;j++){
            cout<<a[i][j]<<" ";
        }
        cout<<endl;
    }
}
int main()
{
    int t;
    cin>>t;
    while(t--){
    int n,m;
    cin>>n>>m;
    int a[230][230];
    nhapMaTran2Chieu(a,n,m);
    for(int j=1;j<=m;j++){
        for(int i=2;i<=n;i++){
            if(a[i][j]>0) { a[i][j]=a[i-1][j]+1;}
        }
    }
    for(int i=1;i<=n;i++){
        for(int j=1;j<=m;j++){
            for(int k=j+1;k<=m;k++){
                if(a[i][j]<a[i][k]){
                    swap(a[i][j],a[i][k]);
                }
            }
        }
    }
    int dientichmax=0;
    for(int i=1;i<=n;i++){
        for(int j=1;j<=m;j++){
            if(dientichmax < j*a[i][j]) dientichmax = j*a[i][j];
        }}
        cout<<dientichmax<<endl;
    }
}
CỬA SỔ TRƯỢT
#include <bits/stdc++.h>

using namespace std;

void scanMatrix(int a[][200], int m){
    for (int i = 0; i < m; i ++){
        for (int j = 0; j < m;j ++){
            cin >> a[i][j];
        }
    }
}
int main (){
    int n, m;
    int a[200][200], b[200][200];
    cin >> n;
    scanMatrix(a, n);
    cin >> m;
    scanMatrix(b, m);
    int h = 0, k = 0;
    for (int i = 0; i < n; i ++){
        for (int j = 0; j < n; j ++){
            cout << a[i][j] * b[h][k] << " ";
            k ++;
            if (k == m) 
                k = 0;
        }
        h ++;
        if (h == m)
            h = 0;
        cout << "\n";
    }
}
